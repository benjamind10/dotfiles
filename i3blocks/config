[cpu_usage]
command=$HOME/.config/i3blocks/scripts/cpu_usage.sh
interval=cpu_usage.sh
interval=10
#color=#00FF00

[ram_usage]
command=$HOME/.config/i3blocks/scripts/memory_usage.sh
interval=10
#color=#FFFF00

